{
    "name": "Particle Electron",
    "based_on": "https://github.com/spark/firmware/blob/develop/hal/inc/pinmap_hal.h",
    "map": {
        "digital": {
            "pins": {
                "D0":  0,  "D1":  1,  "D2":  2,  "D3":  3,  "D4":  4,
                "D5":  5,  "D6":  6,  "D7":  7,
                "B0": 24,  "B1": 25,  "B2": 26,  "B3": 27,  "B4": 28,  "B5": 29,
                "C0": 30,  "C1": 31,  "C2": 32,  "C3": 33,  "C4": 34,  "C5": 35,
                "RX": 18,  "TX": 19,
                "BTN":20
            },
            "ops": [ "dr", "dw" ]
        },
        "analog": {
            "pins": {
                "DAC": 16, "WKP": 17,
                "A0":  10, "A1":  11, "A2":  12, "A3":  13, "A4":  14, "A5":  15,
                                      "B2":  26, "B3":  27, "B4":  28, "B5":  29
            },
            "ops": [ "dr", "dw", "ar" ],
            "arRange": [0, 4095]
        },
        "pwm": {
            "pins": [
                                        "A4", "A5",
                "B0", "B1", "B2", "B3",
                                        "C4", "C5",
                "D0", "D1", "D2", "D3",
                "WKP", "RX", "TX"
            ],
            "ops": [ "aw" ],
            "awRange": [0, 255]
        },
        "virtual":  {
            "pinsRange": [ 0, 127 ],
            "ops": [ "vr", "vw" ]
        }
    }
}
