{
    "name": "Arduino MKR1000",
    "map": {
        "digital": {
            "pins": {
                "D0":  0,  "D1":  1,  "D2":  2,  "D3":  3,  "D4":  4,
                "D5":  5,  "D6":  6,  "D7":  7,  "D8":  8,  "D9":  9,
                "D10": 10, "D11": 11, "D12": 12, "D13": 13, "D14": 14
            },
            "ops": [ "dr", "dw" ]
        },
        "analog": {
            "pins": {
                "A0": 15, "A1": 16, "A2": 17, "A3": 18, "A4": 19, "A5": 20, "A6": 21
            },
            "ops": [ "dr", "dw", "ar" ],
            "arRange":[0, 1023]
        },
        "pwm": {
            "pins": [
                "D0", "D1", "D2", "D3", "D4", "D5", "D6", "D7", "D8", "D10",
                "A0",             "A3", "A4"
            ],
            "ops": [ "aw" ],
            "awRange":[0, 255]
        },
        "virtual":  {
            "pinsRange": [ 0, 127 ],
            "ops": [ "vr", "vw" ]
        }
    }
}
