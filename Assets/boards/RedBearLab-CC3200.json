{
    "name": "RedBearLab CC3200/Mini",
    "based_on": "https://github.com/energia/Energia/blob/master/hardware/cc3200/variants/RedBearLab_WiFiMini/pins_energia.h",
    "map": {
        "digital": {
            "pins": {
                "D0":  0,  "D1":  1,  "D2":  2,  "D3":  3,  "D4": 4,
                "D5":  5,  "D6":  6,  "D7":  7,  "D8":  8,  "D9": 9,
                "D10": 10, "D11": 11, "D12": 12, "D13": 13, "D14": 14,
                "D15": 15,
                "D20": 20, "D21": 21, "D22": 22, "D23": 23, "D24": 24
            },
            "ops": [ "dr", "dw" ]
        },
        "analog": {
            "pins": {
                "A1": 17, "A2": 18, "A3": 19
            },
            "ops": [ "dr", "dw", "ar" ],
            "arRange": [0, 1023]
        },
        "pwm": {
            "pins": [
                "D5", "D6", "D14", "D15"
            ],
            "ops": [ "aw" ],
            "awRange": [0, 255]
        },
        "virtual":  {
            "pinsRange": [ 0, 127 ],
            "ops": [ "vr", "vw" ]
        }
    }
}
