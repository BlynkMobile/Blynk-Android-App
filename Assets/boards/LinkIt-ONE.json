{
    "name": "LinkIt ONE",
    "map": {
        "digital": {
            "pins": {
                "D0":  0,  "D1":  1,  "D2":  2,  "D3":  3, "D4": 4,
                "D5":  5,  "D6":  6,  "D7":  7,  "D8":  8, "D9": 9,
                "D10": 10, "D11": 11, "D12": 12, "D13": 13,
                "D18": 18, "D19": 19
            },
            "ops": [ "dr", "dw" ]
        },
        "analog": {
            "pins": {
                "A0": 14, "A1": 15, "A2": 16, "A3": 17
            },
            "ops": [ "dr", "dw", "ar" ],
            "arRange": [0, 1023]
        },
        "pwm": {
            "pins": [
                "D3", "D9"
            ],
            "ops": [ "aw" ],
            "awRange": [0, 1023]
        },
        "virtual":  {
            "pinsRange": [ 0, 127 ],
            "ops": [ "vr", "vw" ]
        }
    }
}
